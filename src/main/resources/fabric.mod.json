{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "Syorito Hatsuki"
  ],
  "contact": {
    "issues": "https://github.com/deos-team/entrium-rpg/issues",
    "sources": "https://github.com/deos-team/entrium-rpg"
  },
  "license": "MIT Licence",
  "icon": "assets/${modId}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.deos.${modId}.Entrium"
      }
    ]
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/wUHzyqtD"
      },
      "badges": [],
      "update_checker": true
    },
    "mcb": [
      {
        "name": "Deos Team",
        "fillColor": -13730510,
        "outlineColor": -11751600
      }
    ]
  },
  "mixins": [
    "${modId}.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabricloader": ">=${loaderVersion}",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "minecraft": ">=${minecraftVersion}",
    "java": ">=${javaVersion}"
  },
  "recommends": {
    "modmenu-badges-lib": "*"
  }
}